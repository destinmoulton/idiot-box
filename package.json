{
    "name": "idiot-box",
    "version": "0.0.1",
    "description": "Web based media manager for Raspberry Pi.",
    "main": "index.js",
    "scripts": {
        "clean-server": "rm -rf dist/server/app/*",
        "compile-react": "webpack --config webpack.config.js -d --watch --no-devtool",
        "compile-server": "npm run clean-server && tsc --watch ",
        "profile": "webpack --profile --json > stats.json",
        "start-server": "pm2 start pm2.config.json",
        "test": "jest --silent=false --colors --verbose",
        "test-watch": "jest --silent=false --colors --watchAll",
        "update": "node /home/pi/idiot-box/dist/server/app/scripts/daily_update.js"
    },
    "keywords": [
        "raspberry-pi",
        "react",
        "express"
    ],
    "author": "Destin Moulton",
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.19.3",
        "@babel/core": "^7.20.5",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-react": "^7.18.6",
        "@types/got": "^9.6.12",
        "@types/jest": "^29.2.3",
        "babel-jest": "^29.3.1",
        "babel-loader": "^9.1.0",
        "babel-plugin-import": "^1.13.5",
        "enzyme": "^3.11.0",
        "jest": "^29.3.1",
        "webpack": "^5.75.0",
        "webpack-bundle-analyzer": "^4.7.0",
        "webpack-cli": "^5.0.0"
    },
    "dependencies": {
        "@emotion/react": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "@mui/icons-material": "^5.10.16",
        "@mui/material": "^5.10.16",
        "@types/express": "^4.17.14",
        "cheerio": "^1.0.0-rc.12",
        "express": "^4.18.2",
        "express-basic-auth": "^1.2.1",
        "fs-extra": "^11.1.0",
        "got": "^12.5.3",
        "gulp": "^4.0.2",
        "gulp-sass": "^5.1.0",
        "gulp-watch": "^5.0.1",
        "lodash": "^4.17.21",
        "mkdirp": "^1.0.4",
        "moment": "^2.29.4",
        "node-fetch": "^3.3.0",
        "random-useragent": "^0.5.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-lazy-load-image-component": "^1.5.6",
        "react-redux": "^8.0.5",
        "react-router-dom": "^6.4.4",
        "react-router-redux": "^4.0.8",
        "react-select": "^5.7.0",
        "redux": "^4.2.0",
        "redux-thunk": "^2.4.2",
        "socket.io": "^4.5.4",
        "socket.io-client": "^4.5.4",
        "sqlite": "^4.1.2",
        "sqlite3": "^5.1.2",
        "trakt.tv": "^8.1.1"
    },
    "jest": {
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/src/"
        ]
    }
}
